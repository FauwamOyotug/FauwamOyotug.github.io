  <!-- Treasure Chest -->
  <div class="treasure" onclick="toggleTreasure()">üß≥</div>
  <div class="treasure-popup" id="treasurePopup">
    <div style="font-size:30px;margin-bottom:10px">üéâ You found treasure! üéâ</div>
    <p>Secret skill unlocked: I can hold my breath while debugging for 3 hours straight! ü§ø</p>
